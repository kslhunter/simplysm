{
  "name": "simplysm",
  "version": "11.0.6",
  "description": "심플리즘 패키지",
  "author": "김석래",
  "repository": {
    "type": "git",
    "url": "https://github.com/kslhunter/simplysm11.git"
  },
  "type": "module",
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@3.6.4",
  "engines": {
    "node": "^16"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "watch": "yarn _sd-cli_ watch",
    "watch:sd-angular": "yarn _sd-cli_ --debug watch --packages sd-angular",
    "build": "yarn _sd-cli_ --debug build",
    "publish": "yarn _sd-cli_ --debug publish",
    "--": "",
    "_sd-cli_": "tsx --experimental-import-meta-resolve --tsconfig packages/sd-cli/tsconfig.json packages/sd-cli/src/sd-cli.ts",
    "commit": "git add . && git commit -m .",
    "eslint-timing": "cross-env TIMING=1 eslint \"**/+(*.ts|*.tsx|*.js|*.cjs|*.mjs)\"",
    "eslint-fix": "cross-env TIMING=1 eslint --fix \"**/+(*.ts|*.tsx|*.js|*.cjs|*.mjs)\""
  },
  "devDependencies": {
    "@simplysm/eslint-plugin": "11.0.6",
    "@types/node": "^16.18.58",
    "cross-env": "^7.0.3",
    "eslint": "^8.51.0",
    "tsx": "^3.13.0",
    "typescript": "^4.9.5"
  }
}
