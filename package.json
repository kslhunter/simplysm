{
  "name": "simplysm",
  "version": "13.0.69",
  "description": "Simplysm packages",
  "author": "김석래",
  "repository": {
    "type": "git",
    "url": "https://github.com/kslhunter/simplysm.git"
  },
  "type": "module",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "watch": "pnpm sd-cli watch",
    "dev": "pnpm sd-cli dev",
    "build": "pnpm sd-cli build",
    "pub": "pnpm sd-cli publish",
    "pub:no-build": "pnpm sd-cli --no-build",
    "---": "",
    "typecheck": "pnpm sd-cli typecheck",
    "lint": "pnpm sd-cli lint",
    "lint:fix": "pnpm sd-cli lint --fix",
    "check": "pnpm sd-cli check",
    "vitest": "vitest",
    "----": "",
    "sd-cli": "cross-env NODE_OPTIONS=--max-old-space-size=8192 tsx packages/sd-cli/src/sd-cli.ts",
    "sd-claude": "cross-env tsx packages/sd-claude/src/sd-claude.ts",
    "reinstall": "pnpm install --force",
    "postinstall": "playwright install"
  },
  "devDependencies": {
    "@solidjs/testing-library": "^0.8.10",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/node": "^20.19.33",
    "@vanilla-extract/vite-plugin": "^5.1.4",
    "@vitest/browser": "^4.0.18",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/coverage-v8": "^4.0.18",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.3",
    "jiti": "^2.6.1",
    "playwright": "^1.58.2",
    "prettier": "^3.8.1",
    "solid-js": "^1.9.11",
    "stylelint": "^16.26.1",
    "tailwindcss": "^3.4.19",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-solid": "^2.11.10",
    "vite-tsconfig-paths": "^6.1.1",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "esbuild-plugin-solid": "^0.6.0"
  }
}
