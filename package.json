{
  "name": "simplysm",
  "version": "7.0.150",
  "description": "심플리즘 패키지",
  "author": "김석래",
  "repository": {
    "type": "git",
    "url": "https://github.com/kslhunter/simplysm7.git"
  },
  "type": "module",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "^16"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "watch": "npm run _sd-cli_ -- watch",
    "build": "npm run _sd-cli_ -- build",
    "----- device": "",
    "run-electron:devtool": "npm run _sd-cli_ -- run-electron sd-devtool http://localhost:60280",
    "----- debug": "",
    "watch(--debug)": "npm run _sd-cli_ -- --debug watch",
    "build(--debug)": "npm run _sd-cli_ -- --debug build",
    "----- pkg(--debug)": "",
    "watch:devtool(--debug)": "npm run _sd-cli_ -- --debug watch --packages sd-devtool",
    "build:devtool(--debug)": "npm run _sd-cli_ -- --debug build --packages sd-devtool",
    "----- ksl": "",
    "ksl:publish": "npm run _sd-cli_ -- publish --config simplysm-ksl.json",
    "ksl:publish(--debug)(--noBuild)": "npm run _sd-cli_ -- --debug publish --noBuild -- config simplysm-ksl.json",
    "ksl:enc_conf": "npm run _sd-cli_ -- enc-file simplysm-ksl.json",
    "ksl:dec_conf": "npm run _sd-cli_ -- dec-file simplysm-ksl.json.enc",
    "----- ksl:pkg(--debug)": "",
    "ksl:publish:devtool(--debug)(--noBuild)": "npm run _sd-cli_ -- --debug publish --noBuild --packages sd-devtool --config simplysm-ksl.json",
    "----- utils": "",
    "update": "npm run _sd-cli_ -- update",
    "postinstall": "npm run _sd-cli_ -- prepare",
    "---": "",
    "_sd-cli_": "cross-env TS_NODE_TRANSPILE_ONLY=true cross-env TS_NODE_PROJECT=packages/sd-cli/tsconfig.json node --max-old-space-size=8192 --experimental-specifier-resolution=node --experimental-import-meta-resolve --loader ./packages/sd-cli/lib/ts-node-esm-paths packages/sd-cli/src/bin/sd-cli.ts",
    "eslint-timing": "cross-env TIMING=1 eslint \"**/+(*.ts|*.js|*.cjs|*.mjs)\""
  },
  "devDependencies": {
    "@types/node": "^17.0.12",
    "cross-env": "^7.0.3",
    "typescript": "~4.5.5",
    "eslint": "^8.7.0",
    "mocha": "^9.2.0",
    "chai": "^4.3.6",
    "@types/mocha": "^9.1.0",
    "@types/chai": "^4.3.0"
  }
}