{
  "name": "simplysm",
  "version": "3.4.81",
  "description": "심플리즘 패키지",
  "author": "김석래",
  "repository": "github:kslhunter/simplysm",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*",
    "types/*",
    "test"
  ],
  "scripts": {
    "-- watch": "",
    "build:watch": "yarn _sd-cli_ build --watch",
    "-- no watch": "",
    "build": "yarn _sd-cli_ build",
    "publish": "yarn _sd-cli_ publish",
    "publish:build": "yarn _sd-cli_ publish --build",
    "-- debug": "",
    "debug:build:watch": "yarn _sd-cli_ --debug build --watch",
    "debug:build": "yarn _sd-cli_ --debug build",
    "debug:publish": "yarn _sd-cli_ --debug publish",
    "debug:publish:build": "yarn _sd-cli_ --debug publish --build",
    "-- debug for package": "",
    "debug:build:watch:sd-angular": "yarn _sd-cli_ --debug build --watch --packages @simplysm/sd-angular",
    "debug:build:watch:sd-core-common": "yarn _sd-cli_ --debug build --watch --packages @simplysm/sd-core-common",
    "----": "",
    "_sd-cli_": "cross-env TS_NODE_TRANSPILE_ONLY=true cross-env TS_NODE_PROJECT=packages/sd-cli/tsconfig.json node --require=ts-node/register --require=tsconfig-paths/register packages/sd-cli/src/bin.ts"
  },
  "devDependencies": {
    "@simplysm/eslint-plugin": "3.4.81",
    "@types/node": "^14.0.14",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/eslint-plugin-tslint": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.3.1",
    "eslint-plugin-import": "^2.22.0",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.2",
    "typescript": "^3.9.5"
  }
}