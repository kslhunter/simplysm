{
  "name": "simplysm",
  "version": "6.2.0",
  "description": "심플리즘 패키지",
  "author": "김석래",
  "repository": "github:kslhunter/simplysm6",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "watch": "npm run _sd-cli_ watch",
    "_sd-cli_": "cross-env TS_NODE_TRANSPILE_ONLY=true cross-env TS_NODE_PROJECT=packages/sd-cli/tsconfig.json node --experimental-json-modules --experimental-specifier-resolution=node --loader=./packages/sd-cli/esm.mjs packages/sd-cli/src/bin/sd-cli.ts"
  },
  "devDependencies": {
    "@simplysm/eslint-plugin": "6.2.0",
    "@types/node": "^16.11.7",
    "cross-env": "^7.0.3",
    "eslint": "8.2.0",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "~4.4.4"
  }
}
