<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<plugin id="@simplysm/cordova-plugin-usb-storage" xmlns="http://apache.org/cordova/ns/plugins/1.0" version="12.10.15">
  <name>USB Storage</name>
  <description>Cordova USB Storage Plugin</description>
  <license>Apache 2.0</license>
  <keywords>cordova,plugin,usb,storage</keywords>
  <platform name="android">
    <source-file src="android/CordovaUsbStorage.java" target-dir="src/kr/co/simplysm/cordova"></source-file>
    <config-file target="res/xml/config.xml" parent="/*">
      <feature name="CordovaUsbStorage">
        <param name="android-package" value="kr.co.simplysm.cordova.CordovaUsbStorage"></param>
      </feature>
    </config-file>
    <framework src="me.jahnen.libaums:core:0.9.1"></framework>
  </platform>
</plugin>
