<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0" id="@simplysm/cordova-plugin-auto-update"
        version="12.10.3">
  <name>Auto Update</name>
  <description>Cordova Auto Update Plugin</description>
  <license>Apache 2.0</license>
  <keywords>cordova,plugin,auto,update</keywords>
  <platform name="android">
    <dependency id="@simplysm/cordova-plugin-file-system"/>
    <dependency id="cordova-plugin-file-opener2"/>
    <dependency id="cordova-plugin-android-permissions"/>

    <config-file target="app/src/main/AndroidManifest.xml" parent="/*">
      <uses-permission android:name="android.permission.REQUEST_INSTALL_PACKAGES"/>
    </config-file>
  </platform>
</plugin>
